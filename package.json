{
  "name": "ai-photo-gallery",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "overrides": {
    "jsdom": {
      "parse5": "7.1.2"
    }
  },
  "scripts": {
    "// ğŸš€ Development": "",
    "dev": "vite",
    "dev:frontend": "vite",
    "dev:server": "cd server && npm run dev",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "// ğŸ§ª Testing": "",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:local-cloudrun": "npm run build && docker build -f Dockerfile.optimized -t local-gallery . && docker run -p 3000:3000 --env-file env.local.example local-gallery",
    "test:stress": "node test-system-stress.cjs",
    "test:load": "node test-load-realistic.cjs",
    "// ğŸ” Linting & Formatting": "",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "type-check": "tsc --noEmit",
    "// ğŸ³ Docker Development": "",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:docker:clean": "docker-compose -f docker-compose.dev.yml down -v",
    "dev:docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "// ğŸš€ Deploy": "",
    "predeploy": "./pre-deploy-check.sh",
    "deploy:fast": "gcloud builds submit --config cloudbuild.yaml",
    "deploy:direct": "gcloud run deploy ai-photo-gallery --source . --region us-west1 --allow-unauthenticated",
    "// ğŸ”¥ Firebase": "",
    "firebase:start": "firebase emulators:start --only firestore,storage,functions,auth",
    "firebase:export": "firebase emulators:export ./firebase-export",
    "firebase:import": "firebase emulators:start --import ./firebase-export",
    "// ğŸ§¹ Utilities": "",
    "clean": "rm -rf dist node_modules/.vite .cache",
    "clean:all": "rm -rf dist node_modules server/node_modules functions/node_modules .cache",
    "clean:docker": "docker system prune -f && docker volume prune -f",
    "setup:env": "cp env.local.example .env.local",
    "setup:install": "npm install && cd server && npm install && cd ../functions && npm install",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/genai": "^1.23.0",
    "@sentry/react": "^10.32.1",
    "@stripe/stripe-js": "^8.5.1",
    "ai": "^5.0.76",
    "browser-image-compression": "^2.0.2",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "emoji-picker-react": "^4.15.1",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^6.5.0",
    "firebase-tools": "^14.20.0",
    "framer-motion": "^12.23.24",
    "intro.js": "^8.3.2",
    "intro.js-react": "^1.0.0",
    "lucide-react": "^0.554.0",
    "open": "^10.2.0",
    "react": "^19.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-masonry-css": "^1.0.16",
    "react-router-dom": "^7.9.4",
    "stripe": "^19.3.1",
    "yet-another-react-lightbox": "^3.25.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.12",
    "@vitest/ui": "^4.0.10",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.10"
  }
}
